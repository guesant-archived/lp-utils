{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./build",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
